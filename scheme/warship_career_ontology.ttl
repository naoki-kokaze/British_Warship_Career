#################################################################
#    Object Properties - Custom (:warship)
#################################################################

# Subproperties of crm:P2_has_type
:has_operational_status a owl:ObjectProperty ;
    rdfs:subPropertyOf crm:P2_has_type ;
    rdfs:domain crm:E93_Presence ;
    rdfs:range :OperationalStatus ;
    rdfs:comment "When representing a ship's career using E93_Presence, limit the assignment of :has_operational_status to one per Presence. If the operational status changes, the Presence must be split. The temporal boundaries of each respective Presence should be linked using crm:P164 and crm:P182. A period that includes repairs must be modeled as a separate Presence (e.g., :UnderRepair) and must not be included in the same Presence as :InOrdinary."@en .

:has_propulsion_type a owl:ObjectProperty ;
    rdfs:subPropertyOf crm:P2_has_type ;
    rdfs:domain crm:E93_Presence ;
    rdfs:range :PropulsionType ;
    rdfs:comment "Specifies the primary means of propulsion for the ship during a specific presence."@en .

:has_ship_rating a owl:ObjectProperty ;
    rdfs:subPropertyOf crm:P2_has_type ;
    rdfs:domain crm:E93_Presence ;
    rdfs:range :ShipRating .

:has_ship_role a owl:ObjectProperty ;
    rdfs:subPropertyOf crm:P2_has_type ;
    rdfs:domain crm:E93_Presence ;
    rdfs:range :ShipRole ;
    rdfs:comment "Within a single Presence, :has_ship_role and :has_ship_type must not be assigned simultaneously. If a specific non-combat operational role is defined (e.g., :HospitalShip, :GuardShip), use :has_ship_role and omit :has_ship_type, which is used to express its general function as a warship."@en .

:has_ship_type a owl:ObjectProperty ;
    rdfs:subPropertyOf crm:P2_has_type ;
    rdfs:domain crm:E93_Presence ;
    rdfs:range :ShipType ;
    rdfs:comment "Specifies the fundamental design/tactical classification of the ship during a specific presence."@en .

# Subproperties of crm:P31_has_modified
:modifies_ship a owl:ObjectProperty ;
  rdfs:label "modifies ship"@en ;
  rdfs:comment "Generic link from an E11_Modification event to the ship it modifies."@en ;
  rdfs:subPropertyOf crm:P31_has_modified ;
  rdfs:domain crm:E11_Modification ;
  rdfs:range  sealit:Ship .

:converts_into a owl:ObjectProperty ;
  rdfs:label "converts into (assigns new ship role in conversion context)"@en ;
  rdfs:comment "Use alongside the physical modification link (:modifies_ship). For typing/role assignment itself, E13_Attribute_Assignment with P140/P141 is the canonical pattern."@en ;
  rdfs:domain :ShipConversion ;
  rdfs:range  :ShipRole .

:incurred_cost a owl:ObjectProperty ;
  rdfs:label "incurred cost"@en ;
  rdfs:comment "Cost magnitude (E97_Monetary_Amount) incurred by an activity (typically an E11_Modification)."@en ;
  rdfs:subPropertyOf crm:P43_has_dimension ;
  rdfs:domain crm:E5_Event ;
  rdfs:range  crm:E97_Monetary_Amount .

# Subproperties of crm:P141_assigned
#:changes_operational_status a owl:ObjectProperty ;
#    rdfs:subPropertyOf crm:P141_assigned ;
#    rdfs:domain crm:E7_Activity ;
#    rdfs:range :OperationalStatus .

:changes_ship_rating a owl:ObjectProperty ;
    rdfs:subPropertyOf crm:P141_assigned ;
    rdfs:domain crm:E13_Attribute_Assignment ;
    rdfs:range  :ShipRating ;
    rdfs:comment "Use on E13_Attribute_Assignment that assigns a new :ShipRating to a ship; subsequent E93 presences must reflect the new rating prospectively only."@en .


