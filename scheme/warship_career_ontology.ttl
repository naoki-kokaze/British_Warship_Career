crm:P31_has_modified a owl:ObjectProperty ;
                                                         rdfs:subPropertyOf owl:topObjectProperty ;
                                                         rdfs:domain :ShipConversion ;
                                                         rdfs:range sealit:Ship .

crm:P166i_had_presence a owl:ObjectProperty ;
